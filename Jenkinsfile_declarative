
pipeline{
    agent any

    environment{
        PATH = "//opt/maven-3/bin:$PATH"

    }
    
    stages{
        stage("Git Checkout"){
           steps{
               git 'https://github.com/sreemural/my-app.git'
           } 
        }
     stage("Maven build"){
        steps{
            sh "mvn clean package"
        }

           
    }
  }
}
